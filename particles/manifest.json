{
  "manifest_version": 2,
  "name": "particles new tab",
  "author": "Andreas Bigger",
  "version": "1.2.0",
  "description": "Basic particles tab for Google chrome",
  "incognito": "split",
  "icons": {
    "16": "img/16.png",
    "48": "img/48.png",
    "128": "img/128.png"
  },
  "chrome_url_overrides": {
    "newtab": "index.html"
  },
  "permissions": ["storage", "tabs"]
}
